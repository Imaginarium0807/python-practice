
solution 1 3980 ms
class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        ls = len(nums)
        for i in range(ls):
            for j in range(i + 1, ls):
                if nums[i] + nums[j] == target:
                    return[i,j]
                    
                   
                   
